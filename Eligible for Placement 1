import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code 
        here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        int n=sc.nextInt();
        int t=sc.nextInt();
        if(n==1 &&t>70){
            System.out.println("Name of the Student:"+s);
            System.out.println(s+" is Eligible for Placement");
            
        }
        else if(n==1||n==2 && t>75){
            System.out.println("Name of the Student:"+s);
            System.out.println(s+" is Eligible for Placement");

            
            
            
        }
        else{
            System.out.println("Name of the Student:"+s);
            System.out.println(s+" is Not Eligible for Placement");

            
        }
    }
}
