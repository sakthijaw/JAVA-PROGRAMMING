import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int res=n;
	    int count=0;
	    while(n>0){
	        n=n/10;
	        count++;
	    }
	    
	   while(res>0){
	       int c=res%10;
	       System.out.println("Digit in "+count+"th position is "+c);
	       res=res/10;
	       count--;
	       
	   }
	    
	   
	
	}
}
